{
  "extends": "airbnb",
  "rules" : {
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "no-multiple-empty-lines": 0,
    "no-unused-vars": ["error", { "varsIgnorePattern": "coMocha" }],
    "strict": 0,
    "max-len": 0,
    "import/no-dynamic-require": 0,
    "arrow-parens": 0,
    "import/newline-after-import": 0
  }
}